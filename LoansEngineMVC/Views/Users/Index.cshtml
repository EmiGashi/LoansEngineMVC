@model IEnumerable<LoansEngineMVC.Models.User>

<h1>List of Users Registered</h1>

<table class="table table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Company</th>
            <th>Title</th>
            <th>Comment</th>
            <th>Webinar</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.EmailAddress</td>
                <td>@user.Company</td>
                <td>@user.Title</td>
                <td>@user.Comment</td>
                <td>@user.Webinar</td>
                <td><button class="btn btn-danger"><a href="Users/UpdateUser/@user.Id" />View</button></td>
            </tr>
        }
    </tbody>
</table>